<Window x:Class="KCK_APP.WPF.EditCarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Edytuj Samochód" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="Edytuj Samochód" FontSize="30" HorizontalAlignment="Center" Margin="20"/>
            <TextBox Name="IdTextBox" IsEnabled="False" Margin="5" />
            <TextBox Name="MakeTextBox" Margin="5" />
            <TextBox Name="ModelTextBox" Margin="5" />
            <TextBox Name="PriceTextBox" Margin="5" />
            <Button Content="Zapisz" Click="SaveCar_Click" Margin="10"/>
            <Button Content="Anuluj" Click="Cancel_Click" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>